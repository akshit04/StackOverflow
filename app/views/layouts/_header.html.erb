<header class="navbar navbar-fixed-top navbar-inverse">
 	<div class="container">
	  <%= link_to "StackOverflow", root_path %>
	  <nav>
	    <ul class="nav navbar-nav navbar-right">
	      <li><%= link_to "Home",   root_path %></li>
	      <% if logged_in? %>
	        <li><%= link_to "Users", users_path %></li>
	        <li><%= link_to "Profile", current_user %></li>   <!-- current_user is equivalent to user_path(current_user) -->
	        <li><%= link_to "Edit", edit_user_path(current_user) %></li> <!-- path for /users/1/edit obtained from resources :user-->
	        <li><%= link_to "My Questions", user_questions_path(current_user) %></li>
	        <li>
	         	<%= link_to "Log out", logout_path, method: "delete" %>
	        </li>
	      <% else %>
	      	<li><%= link_to 'SignUp', new_user_path %></li>
	       	<li><%= link_to "Log in", login_path %></li>
	       <% end %>
			</ul> 
			<br><br><br>
		</nav>
	</div>
</header>